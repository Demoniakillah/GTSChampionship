    <h1>Race Car Configuration</h1>

    <a href="{{ path('race_car_configuration_new') }}">Create new</a>
    <table class="table">
        <thead>
            <tr>
                <th>Race</th>
                <th>Car</th>
                <th>Parameter</th>
                <th>Value</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for race_car_configuration in race_car_configurations %}
            <tr>
                <td>{{ race_car_configuration.race }}</td>
                <td>{{ race_car_configuration.car }}</td>
                <td>{{ race_car_configuration.parameter }}</td>
                <td>{{ race_car_configuration.value }}</td>
                <td>
                    <a href="{{ path('race_car_configuration_show', {'id': race_car_configuration.id}) }}">show</a>
                    <a href="{{ path('race_car_configuration_edit', {'id': race_car_configuration.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

